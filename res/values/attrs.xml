<?xml version="1.0" encoding="utf-8"?>

<resources xmlns:android="http://schemas.android.com/apk/res/android">  
  <!-- CellLayout 单个屏幕布局 -->  
  <declare-styleable name="DocIndicator"> 
    <attr name="lightImage" format="integer"/>  
    <attr name="darkImage" format="integer"/>  
    <attr name="dotWidth" format="dimension"/>  
    <attr name="total" format="integer"/>  
    <attr name="current" format="integer"/>  
    <attr name="dotPadding" format="dimension"/> 
  </declare-styleable>  

  <declare-styleable name="StaggeredGridView">
        <attr name="drawSelectorOnTop" format="boolean" />
        <attr name="numColumns" format="integer" />
        <attr name="itemMargin" format="dimension" />
    </declare-styleable>
    
  <declare-styleable name="pullImage">
        <attr name="initHeight" format="integer" />
    </declare-styleable>
    
</resources>
